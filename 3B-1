#include <iostream>

using namespace std;

int Funcion(int x) {
	int Resultado;
	Resultado = (x <= 0) ? pow(x, 6) + x - 2 : (x > 0 && x < 4) ? sqrt(pow(x, 3)) + 4 * x + 1 : pow(x, 2) + 1;
	return Resultado;
}

int main() {
	int x, Resultado;
	cout << "Ingrese x: ";
	cin >> x;
	Resultado = Funcion(x);
	cout << "El resultado despues de evaluarlo en la funcion es: " << Resultado;

	return 0;
}
